{
	"name": "@onedotprojects/eslint-plugin",
	"version": "1.0.0",
	"description": "OneDot Projects' ESLint Style Guide",
	"keywords": [
		"eslint",
		"eslint-plugin",
		"eslint-config"
	],
	"homepage": "https://github.com/onedotprojects/eslint-plugin#readme",
	"bugs": {
		"url": "https://github.com/onedotprojects/eslint-plugin/issues",
		"email": "1auth.wgyt@gmail.com"
	},
	"license": "MIT",
	"author": "OneDot Projects",
	"main": "index.js",
	"scripts": {
		"lint": "prettier --ignore-path .gitignore --write ."
	},
	"prettier": {
		"printWidth": 80,
		"proseWrap": "always",
		"quoteProps": "consistent",
		"tabWidth": 4,
		"trailingComma": "all",
		"useTabs": true
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/onedotprojects/eslint-plugin.git"
	},
	"dependencies": {
		"eslint-config-hardcore": "^19.8.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-compat": "^3.9.0",
		"eslint-plugin-eslint-comments": "^3.2.0",
		"eslint-plugin-html": "^6.1.2",
		"eslint-plugin-import": "^2.23.4",
		"eslint-plugin-jsdoc": "^35.1.3",
		"eslint-plugin-markdown": "^2.2.0",
		"eslint-plugin-node": "^11.1.0",
		"eslint-plugin-optimize-regex": "^1.2.0",
		"eslint-plugin-promise": "^5.1.0",
		"eslint-plugin-regexp": "^0.12.1",
		"eslint-plugin-switch-case": "^1.1.2",
		"eslint-plugin-unicorn": "^33.0.1"
	},
	"peerDependencies": {
		"eslint": "^7.28.0",
		"prettier": "^2.3.1"
	}
}
